" Note:
"   gvim is aliased in my zshrc in Mac OS X
set editor='zsh -lc "gvim -f \$*" gvim '

" Use <A-i> to launch the external editor
inoremap <A-i> <C-i>

ignorekeys clear
ignorekeys add localhost:9999
ignorekeys add localhost:8888
ignorekeys add localhost:8000
ignorekeys add lambdalisue.github.io/rehighlight
ignorekeys add nup-lpmns
ignorekeys add karyo-lpmns
