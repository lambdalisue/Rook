" vim -u ~/.vim/vimrc.min
if has('vim_starting')
  execute 'set runtimepath^=~/.cache/dein/repos/github.com/Shougo/dein.vim'
endif

if dein#load_state('~/.cache/dein')
  call dein#begin('~/.cache/dein')
  call dein#add('Shougo/denite.nvim')
  call dein#end()
  call dein#save_state()
endif

filetype plugin indent on
syntax on

" Personal preference
nnoremap ; :
nnoremap : ;
