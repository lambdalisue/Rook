{
  "title": "VirtualBox.app",
  "rules": [
    {
      "description": "Replace Eisuu/Kana (lang2/lang1) -> Ctrl+Shift+A/Z in VirtualBoxVM",
      "manipulators": [
        {
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org\\.virtualbox\\.app\\.VirtualBoxVM$"
              ]
            }
          ],
          "from": {
            "key_code": "lang2"
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": [
                "left_shift",
                "left_control"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org\\.virtualbox\\.app\\.VirtualBoxVM$"
              ]
            }
          ],
          "from": {
            "key_code": "lang1"
          },
          "to": [
            {
              "key_code": "z",
              "modifiers": [
                "left_shift",
                "left_control"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}

